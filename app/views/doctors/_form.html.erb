<%= form_for @doctor do |f|%>
<%= f.label :first_name%><br>
<%= f.text_field :first_name%><br>
<%= f.label :last_name%><br>
<%= f.text_field :last_name%><br>
<%= f.label :gender%><br>
<%= f.text_field :gender%><br>
<%= f.label :birthday%><br>
<%= f.date_field :birthday%><br>
<%= f.label :language%><br>
<%= f.text_field :language%><br>
<%= f.label :city%><br>
<%= f.text_field :city%><br>
<%= f.label :country%><br>
<%= f.text_field :country%><br>
<%= f.label :admission_id%><br>
<%= f.text_field :admission_id%><br>
<%= f.label :certification%><br>
<%= f.text_field :certification%><br>
<p> <%= f.submit "Submit", data: {confirm: "Create Profile?"}%> </p>
<% end %>

