<div class="col-12" style="text-align: center; width: 100%">
   <div class="row" style="width: 100%; text-align: center">
      <div class="col-10" style="margin-left: auto; margin-right: auto;">
         <br>

         <h1>Edit Patient Profile</h1>
          <br>
         <br>
         <br>
         <div style="border-style: solid; border-width: 1px;">
            <%= form_for @patient do |form| %>
               <%= render partial: "patients/form", object: form %>
               <div class="submit-field">
                  <%= form.submit "Update Your Profile", data: {confirm: "Are you sure all your imformation is correct?"}, class: "btn btn-outline-dark" %>
               </div>
               <br>
            <% end %>
         </div>
      </div>
   </div>
</div>