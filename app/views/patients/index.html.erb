<div id="search-form">
    <%= form_tag patients_search_path, method: :get do %>
      <%= label_tag "search" %>
      <%= text_field_tag "search", nil, list: "list" %>
      <datalist id="list"></datalist>
    <%end%>
</div>

<br>
<%= link_to "Create a new health profile", new_patient_health_profile_path(@patient) %> 
<br>

<div>
  <h4>Your Notifications</h4>
  <% if !@patient.notifications.empty? %>
    <div>
      <% @patient.notifications.each do |note| %>
        <p>
          <span><%= note.text %></span>
          <span><%= link_to "View more here", url_for(Rails.application.routes.recognize_path(note.link)) %></span>
        </p>
      <% end %>
    </div>
  <% else %>
    You have no new notifications.
  <% end %>
</div>

