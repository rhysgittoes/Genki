<table>
<thead>
  <th>No.</th>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Admission ID</th>
  <th>Email</th>
</thead>
<tbody>
Doctor result :<%= @result.size %>
<% @result.each_with_index do |user, index| %>
<td><%= index + 1%></td>
<td><%= link_to user.first_name, doctor_path(user) %></td>
<td><%= link_to user.last_name, doctor_path(user) %></td> 
<td><%= link_to user.admission_id, doctor_path(user) %></td> 
<td><%= user.email %></td> 
 </tbody>
<%end%>
</tbody>
</table>