 health_profile
<%#= form_for @health_profile, :url => url_for(:controller => "health_profiles_controller", :action => "create") do |f|%>
<%= form_for [@patient, @health_profile] do |f|%>
<%= f.label :blood_type%><br>
<%= f.text_field :blood_type%><br>
<%= f.label :weight %><br>
<%= f.text_field :weight%><br>
<%= f.label :height%><br>
<%= f.text_field :height%><br>
<%= f.label :insurer%><br>
<%= f.text_field :insurer%><br>

<%= f.submit "Submit", data: {confirm: "Create Profile?"}%>
<% end %>